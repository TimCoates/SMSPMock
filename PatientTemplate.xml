<?xml version="1.0" encoding="UTF-8"?><Patient xmlns="http://hl7.org/fhir">
	<id value="example"/>
	<text><status value="generated"/><div xmlns="http://www.w3.org/1999/xhtml"></div></text>
	<identifier>
		<use value="usual"/>
		<type>
			<coding>
				<system value="https://fhir.nhs.uk/Id/nhs-number"/>
				<code value="{{nhs_number}}"/>
			</coding>
		</type>
	</identifier>
	<active value="true"/>
	<name>
		<use value="official"/>
		<family value="{{family_name}}"/>
		<given value="{{given_name}}"/>
        <prefix value="{{title}}" />
	</name>
	<telecom>{{telecom}}</telecom>
	<gender value="{{gender}}"/>
	<birthDate value="{{birthdate}}"/>
	<deceasedBoolean value="false"/>
	<address>{{address}}</address>
	<managingOrganization><reference value="https://fhir.nhs.uk/"/></managingOrganization>
</Patient>